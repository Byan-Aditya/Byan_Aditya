{
  "name": "project-ku",
  "version": "1.0.0",
  "scripts": {
    "build:js": "javascript-obfuscator js/ --output dist/js/",
    "build:html": "html-minifier-terser index.html --collapse-whitespace --remove-comments --minify-js true --minify-css true -o dist/index.html",
    "build:css": "cleancss -o dist/css/styles.min.css css/*.css",
    "build": "npm run build:js && npm run build:html && npm run build:css"
  },
  "dependencies": {
    "javascript-obfuscator": "^4.0.0",
    "html-minifier-terser": "^7.2.0",
    "clean-css-cli": "^5.7.0"
  }
}
